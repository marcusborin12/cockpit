# Configuração específica para testes automatizados
# Este arquivo deve ser usado apenas durante a execução de testes

# Habilitar botão de credenciais de teste
VITE_ENABLE_TEST_CREDENTIALS=true

# Credenciais de teste para automação
VITE_TEST_USERNAME=test_user
VITE_TEST_PASSWORD=test_password

# URL da API para testes (pode usar mock ou servidor de teste)
VITE_AWX_API=http://localhost:8080

# Configurações de cache para testes (TTL mais baixo)
VITE_CACHE_DASHBOARD_STATS_TTL=1
VITE_CACHE_MONTHLY_DATA_TTL=1
VITE_CACHE_RECENT_EXECUTIONS_TTL=1
VITE_CACHE_VERSION=test